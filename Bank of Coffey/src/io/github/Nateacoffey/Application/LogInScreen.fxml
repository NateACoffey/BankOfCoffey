<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="io.github.Nateacoffey.Application.LogInController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="640.0">
         <children>
            <TextField fx:id="username" layoutX="246.0" layoutY="164.0" />
            <PasswordField fx:id="password" layoutX="246.0" layoutY="217.0" onAction="#userSearch" />
            
            <Button layoutX="289.0" layoutY="349.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="149.0" text="Create a new account" />
            <Text layoutX="246.0" layoutY="160.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter username:" wrappingWidth="149.0" />
            <Text layoutX="246.0" layoutY="213.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter password:" wrappingWidth="149.0" />
            <Text fill="RED" fx:id="userNotFound" layoutX="235.0" layoutY="122.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username/Password not found" visible="false" wrappingWidth="171.0" />
            
         </children>
      </AnchorPane>
   </children>
</VBox>
